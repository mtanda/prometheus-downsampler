{
	"name": "prometheus-downsampler",
	"dockerComposeFile": [
		"./docker-compose.yml"
	],
	"service": "prometheus-downsampler",
	"runServices": [
		"prometheus",
		"prometheus_downsample"
	],
	"workspaceFolder": "/workspace",
	"remoteUser": "vscode",
	"postAttachCommand": ".devcontainer/postAttach.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.Go"
			]
		}
	}
}
